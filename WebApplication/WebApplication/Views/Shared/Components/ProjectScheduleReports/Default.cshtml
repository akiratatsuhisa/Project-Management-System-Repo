@model IEnumerable<ProjectScheduleReport>

<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-12">
            <div class="card card-nav-tabs">
                <h4 class="card-header card-header-info">Nộp lúc: @item.CreatedDate</h4>
                <div class="card-body">
                    <h4 class="card-title">
                        @item.Student.StudentCode - @item.Student.ApplicationUser.LastName @item.Student.ApplicationUser.FirstName
                    </h4>
                    <h6 class="card-text">Ghi chú:</h6>
                    <p class="card-text">@item.Note</p>
                    <a class="btn btn-outline-info" data-toggle="tooltip" data-placement="right" title="@item.ReportUrl" href="@item.ReportUrl" target="_blank">report link</a>
                </div>
            </div>
        </div>
    }
</div>
